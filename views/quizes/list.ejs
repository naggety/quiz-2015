<form method="get" action="/quizes">
  <input type="text" name="search" id="search" placeholder="Busque una pregunta" />
  <input type="submit" value="buscar" />
</form>
<br/>
  <table id="quizes-list">
  <% for (var i = 0; i < quizes.length; i++) { %>
    <tr>
      <td><a href="quizes/<%= quizes[i].id %>"><%= quizes[i].pregunta %></a></td>
      <td><a href="quizes/<%= quizes[i].id %>/edit"><button>editar</button></a></td>
      <td>
        <form method="post" action="quizes/<%= quizes[i].id %>?_method=delete" style="display:inline;">
          <button type="submit" onClick="return confirm('Borrar: <%= quizes[i].pregunta %>');">
            borrar
          </button>
        </form>
      </td>
    </tr>
  <% } %>
  </table>
<br/>
<a href="/quizes/new"><button>Crear pregunta</button></a>
